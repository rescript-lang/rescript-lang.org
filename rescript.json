{
  "name": "rescript-lang.org",
  "namespace": false,
  "jsx": {
    "version": 4
  },
  "dependencies": [
    "@rescript/react",
    "@rescript/webapi"
  ],
  "compiler-flags": [
    "-open WebAPI.Global"
  ],
  "sources": [
    {
      "dir": "app",
      "subdirs": true
    },
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "scripts",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "warnings": {
    "error": "+8",
    "number": "-27-26"
  },
  "suffix": ".mjs",
  "gentypeconfig": {
    "language": "untyped",
    "shims": [],
    "module": "es6"
  }
}
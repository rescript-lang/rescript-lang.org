{
  "name": "rescript-lang.org",
  "namespace": false,
  "jsx": {
    "preserve": true,
    "version": 4
  },
  "dependencies": ["@rescript/react", "@rescript/webapi"],
  "compiler-flags": ["-open WebAPI.Global"],
  "sources": [
    {
      "dir": "app",
      "subdirs": true
    },
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "scripts",
      "subdirs": true
    },
    {
      "dir": "functions",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "warnings": {
    "error": "+8"
  },
  "suffix": ".jsx",
  "gentypeconfig": {
    "language": "untyped",
    "shims": [],
    "module": "es6"
  }
}
